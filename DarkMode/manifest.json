{
  "name": "DarkMode",
  "description": "Dark Mode is a Chrome extension",
  "version": "1.0.0",
  "manifest_version": 2,
  "content_scripts": 
  [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "Content.js" ]
    }
  ],
  "browser_action": 
  {
    "default_popup": "Popup.html",
    "default_title": "Dark Mode"
  }
}